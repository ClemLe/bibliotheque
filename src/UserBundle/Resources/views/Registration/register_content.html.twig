{% trans_default_domain 'FOSUserBundle' %}




 <div class="wrapper">
     
     <h3 class="form-signin-heading">Veuillez remplir tous les champs !</h3>
     <hr class="colorgraph"><br>
     
    {{ form_start(form, {'method': 'post', 'action': path('fos_user_registration_register'), 'attr': {'class': 'fos_user_registration_register'}}) }}
    {{ form_widget(form) }}
    <div>
        <button class="btn btn-lg btn-primary btn-block"  name="Submit" value="Login" type="Submit">{{ 'registration.submit'|trans }}</button>  
    </div>
{{ form_end(form) }}

 </div>
